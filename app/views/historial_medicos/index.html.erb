<p id="notice"><%= notice %></p>

<h1>Historial Medicos</h1>

<table>
  <thead>
    <tr>
      <th>Agenda</th>
      <th>Motivo consulta</th>
      <th>Fecha consulta</th>
      <th>Cdiagnostico</th>
      <th>Tratamiento seguir</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historial_medicos.each do |historial_medico| %>
      <tr>
        <td><%= historial_medico.agenda_id %></td>
        <td><%= historial_medico.motivo_consulta %></td>
        <td><%= historial_medico.fecha_consulta %></td>
        <td><%= historial_medico.cdiagnostico %></td>
        <td><%= historial_medico.tratamiento_seguir %></td>
        <td><%= link_to 'Show', historial_medico %></td>
        <td><%= link_to 'Edit', edit_historial_medico_path(historial_medico) %></td>
        <td><%= link_to 'Destroy', historial_medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

